{
  "name": "redbyte",
  "main": "index.js",
  "version": "0.1.1",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "keywords": [],
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "electron": "^40.0.0",
    "electron-builder": "^26.4.0"
  },
  "build": {
    "appId": "com.redbyte.client",
    "productName": "RedByte",
    "win": {
      "target": "nsis",
      "icon": "assets/icon.ico",
      "requestedExecutionLevel": "requireAdministrator"
    },
    "nsis": {
      "include": "build/installer.nsh"
    },
    "extraResources": [
      {
        "from": "../backend/cmake-build-debug/bin/agent.exe",
        "to": "backend/agent.exe"
      }
    ]
  },
  "dependencies": {
    "dotenv": "^17.2.3"
  }
}
